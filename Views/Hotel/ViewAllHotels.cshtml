            <h1> @p[0].ToString()</h1>
            <p>@p[1].ToString()</p>
            <img src="@p[2].ToString()" style="height:48px;width:48px;" />

            <h1> @p[3].ToString()</h1>
            <p>@p[4].ToString()</p>
            <img src="~/uploads/@ViewBag.cityName/@p[5].ToString()" style="height:48px;width:48px" />
        }*@

    }
</body>
</html>

